{
  "name": "www.beginkit.com",
  "version": "0.8.0",
  "description": "template for s3 aws / heroku app",
  "main": "app/index.js",
  "scripts": {
    "test": "make test"
  },
  "engines": {
    "node": "6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brightdigit/beginkit-template.git"
  },
  "author": "Leo G Dion",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brightdigit/beginkit-template/issues"
  },
  "homepage": "https://github.com/brightdigit/beginkit-template#readme",
  "beginkit": {
    "metadata": {
      "site": {
        "title": "Plazmiq Template",
        "url": "http://template.plazmiq.com"
      }
    },
    "stages": {
      "production": {
        "build": {
          "uglify": true,
          "rev": true
        },
        "server": {
          "port": 8080
        }
      },
      "development": {
        "publishing": {
          "draft": true,
          "future": true
        },
        "server": {
          "port": 8081
        }
      }
    },
    "favicon": {
      "masterPicture": "./static/assets/images/logo.svg",
      "dest": "./.tmp/favicons",
      "iconsPath": "/",
      "design": {
        "ios": {
          "pictureAspect": "backgroundAndMargin",
          "backgroundColor": "#000000",
          "margin": "28%"
        },
        "desktopBrowser": {},
        "windows": {
          "pictureAspect": "noChange",
          "backgroundColor": "#da532c",
          "onConflict": "override"
        },
        "androidChrome": {
          "pictureAspect": "shadow",
          "themeColor": "#ffffff",
          "manifest": {
            "name": "tagmento web",
            "display": "browser",
            "orientation": "notSet",
            "onConflict": "override",
            "declared": true
          }
        },
        "safariPinnedTab": {
          "pictureAspect": "silhouette",
          "themeColor": "#5bbad5"
        }
      },
      "settings": {
        "compression": 5,
        "scalingAlgorithm": "Mitchell",
        "errorOnImageTooSmall": false
      },
      "markupFile": "faviconData.json"
    },
    "fonts": [
      {
        "name": "beginkit",
        "source": "icons",
        "type": "svg",
        "className": "bk"
      },
      {
        "source": "node_modules/font-awesome",
        "name": "font-awesome",
        "stylesheet": "scss/font-awesome.scss"
      },
      {
        "type": "web",
        "name": "Press Start 2P"
      },
      {
        "type": "web",
        "name": "Raleway",
        "weights": [
          400,
          100,
          {
            "weight": 100,
            "styles": [
              "italics"
            ]
          },
          {
            "weight": 500,
            "styles": [
              "italics",
              "normal"
            ]
          }
        ]
      },
      {
        "type": "web",
        "name": "Cutive Mono"
      },
      {
        "source": "static/assets/fonts/swindale",
        "name": "swindale",
        "stylesheet": "swindale.css"
      }
    ],
    "publishing": {
      "type": "github"
    },
    "services": {
      "MailChimp": {
        "folders": {
          "template": "db9b74ea32",
          "files": "3273",
          "campaign": "510b4ee963"
        }
      },
      "Pocket": {
        "sources": [
          {
            "template": "templates/draft.md",
            "parameters": {
              "detailType": "complete",
              "sort": "oldest",
              "tag": "tagmento",
              "state": "archive"
            },
            "path": "posts/drafts",
            "filename": "{{zerofill ordinal 3}}.{{slugify title}}.md"
          }
        ]
      }
    }
  },
  "dependencies": {
    "browserify-shim": "^3.8.12",
    "font-awesome": "fortawesome/Font-Awesome",
    "jquery": "^2.2.0",
    "jquery-placeholder": "^2.3.1",
    "jquery.dropotron": "git+https://github.com/brightdigit/jquery.dropotron.git",
    "jquery.scrollex": "git+https://github.com/brightdigit/jquery.scrollex.git",
    "jquery.scrolly": "git+https://github.com/brightdigit/scrolly.git",
    "skel": "git+https://github.com/brightdigit/skel.git",
    "typed.js": "git+https://github.com/brightdigit/typed.js.git"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery-placeholder": {
      "depends": [
        "jquery:jquery"
      ]
    },
    "jquery.scrolly": {
      "depends": [
        "jquery:jquery"
      ]
    },
    "jquery.dropotron": {
      "depends": [
        "jquery:jquery"
      ]
    },
    "jquery.scrollex": {
      "depends": [
        "jquery:jquery"
      ]
    },
    "typed.js": {
      "depends": [
        "jquery:jquery"
      ]
    }
  }
}
